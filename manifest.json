{
  "name": "tabular",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "icon.png"
  },
   
  "permissions": [ 
    "tabs", 
    "http://*/",
    "https://*/"
  ],
 

  "background": {
    "page": "background.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "ftp://*/*"],
      "all_frames": true,
      "js": ["contentscript.js"],
      "run_at": "document_start"
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}