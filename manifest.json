{
  "name": "tabular",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    // "scripts": ["background.js"],
    "page": "background.html"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "tabular.html"
  }
}